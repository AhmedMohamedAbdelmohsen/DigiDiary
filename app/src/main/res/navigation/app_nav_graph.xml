<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_nav_graph"
    app:startDestination="@id/splashFragment">

    <action
        android:id="@+id/action_activity_to_NewDiaryFragment"
        app:destination="@id/composeMenuItem" />

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.digidiary.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_homeFragment22"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.digidiary.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

    </fragment>
    <fragment
        android:id="@+id/favouritesFragment"
        android:name="com.example.digidiary.FavouritesFragment"
        android:label="fragment_favourites"
        tools:layout="@layout/fragment_favourites">

    </fragment>
    <fragment
        android:id="@+id/composeMenuItem"
        android:name="com.example.digidiary.NewDiaryFragment"
        android:label="fragment_new_diary"
        tools:layout="@layout/fragment_new_diary">

    </fragment>
    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.example.digidiary.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar">

    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.digidiary.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">

    </fragment>
    <fragment
        android:id="@+id/binFragment"
        android:name="com.example.digidiary.BinFragment"
        android:label="fragment_bin"
        tools:layout="@layout/fragment_bin">

    </fragment>
    <fragment
        android:id="@+id/diaryDetailsFragment"
        android:name="com.example.digidiary.DiaryDetailsFragment"
        android:label="fragment_diary_details"
        tools:layout="@layout/fragment_diary_details">

    </fragment>
    <fragment
        android:id="@+id/diaryEditFragment"
        android:name="com.example.digidiary.DiaryEditFragment"
        android:label="fragment_diary_edit"
        tools:layout="@layout/fragment_diary_edit">

    </fragment>
</navigation>